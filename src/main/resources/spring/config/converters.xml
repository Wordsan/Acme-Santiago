 --><beans xmlns="http://www.springframework.org/schema/beans" xmlns:util="http://www.springframework.org/schema/util" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="   http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.0.xsd     http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util-4.0.xsd    ">

	<util:list id="converters">

		<!--Chirp converters-->
		<bean class="converters.StringToChirpConverter"/>
        <bean class="converters.ChirpToStringConverter"/>

		<!--Comment converters-->
		<bean class="converters.StringToCommentConverter"/>
        <bean class="converters.CommentToStringConverter"/>

		<!--ConfigurationSystem converters-->
		<bean class="converters.StringToConfigurationSystemConverter"/>
        <bean class="converters.ConfigurationSystemToStringConverter"/>

		<!--Hike converters-->
		<bean class="converters.StringToHikeConverter"/>
        <bean class="converters.HikeToStringConverter"/>

		<!--Route converters-->
		<bean class="converters.StringToRouteConverter"/>
        <bean class="converters.RouteToStringConverter"/>

		<!--User converters-->
		<bean class="converters.StringToUserConverter"/>
        <bean class="converters.UserToStringConverter"/>

		<!--Agent converters-->
		<bean class="converters.StringToAgentConverter"/>
        <bean class="converters.AgentToStringConverter"/>

		<!--Advertisement converters-->
		<bean class="converters.StringToAdvertisementConverter"/>
        <bean class="converters.AdvertisementToStringConverter"/>
    </util:list>
	
</beans>
